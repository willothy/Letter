extern int printf(char* str);

proc void printInt(int v) {
    printf("%d\n", v);
}

proc void printBool(bool v) {
    if (v == 1) {
        printf("true\n");
    } else {
        printf("false\n");
    }
}

proc void printDouble(double v) {
    printf("%f\n", v);
}

proc void printFloat(float f) {
    let double val = f;
    printf("%f\n", val);
}

proc void print(string str) {
    printf(str);
}