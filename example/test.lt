
#define pi 3.1415926

extern int printf(char* str);
extern double pow(double base, double exponent);

proc int printInt(int v) {
    printf("%d\n", v);
}

proc double printDouble(double v) {
    printf("%f\n", v);
}

proc int main() {
    let int radius = 2;
    let float circumference = 2.0 * pi * radius;
    let double c = circumference;
    printDouble(c);
}
