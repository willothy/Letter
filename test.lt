#dependency "example/print.lt"
#define pi 3.1415926

proc double circumference(double radius) {
    return 2 * pi * radius;
}

proc double circumference(float radius) {
    return 2 * pi * radius;
}

proc float circumference(float radius) {
    return 2 * pi * radius;
}

proc int main() {
    if (1 == 0) {
        printf("yay!");
    } else if (1 == 2) {
        printf("yay 2!");
    } else {
        printf("no!");
    }
    return 0;
}