
#dependency "example/print.lt"
#define pi 3.1415926

proc float circumference(float radius) {
    return 2.0 * pi * radius;
}

proc int main() {
    let double value = 21474836473453453452397.9675656345345;
    printDouble(value);
    let float bigfloat = 21474836473453453452397.9675656345345;
    let double b = bigfloat;
    printf("BigFloat: %f\n", b);
