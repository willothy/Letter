#dependency "example/print.lt"
#define pi 3.1415926

proc double circumferenced(double radius) {
    return 2 * pi * radius;
} 

proc float circumferencef(float radius) {
    return 2 * pi * radius;
}

proc int main() {
    let float value = circumferencef(10.0);
    let double v = value;
    let double d = circumferenced(10.0);
    printDouble(v);
    printDouble(d);
    return 0;
}